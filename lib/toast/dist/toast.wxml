<template name="toast">
    <block wx:if="{{ui.show}}">
        <view class="toast">
            <view class="component-toast {{ui.className}}">
                <!--<view class="icon {{ui.iconName}} "></view>-->
                <view class="text">
                    <text>{{model.message}}</text>
                </view>
            </view>
        </view>
    </block>
</template>
